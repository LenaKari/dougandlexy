{
  "name": "Wedding-Site",
  "version": "1.0.0",
  "description": "A template for React.js",
  "scripts": {
    "dev": "webpack --progress --watch",
    "build": "webpack --progress",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "webpack-dev-server --progress --inline --hot --content-base build --history-api-fallback",
    "start:prod": "webpack --watch --progress && node server.js"
  },
  "dependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "compression": "^1.6.2",
    "css-loader": "^0.24.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "http-server": "^0.8.5",
    "if-env": "^1.0.0",
    "jshint": "^2.9.3",
    "jshint-loader": "^0.8.3",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "node-libs-browser": "^1.0.0",
    "nodemailer": "^2.6.0",
    "postcss-loader": "^0.11.0",
    "react": "^15.1.0",
    "react-bootstrap": "^0.30.3",
    "react-dom": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "react-icons": "^2.2.1",
    "react-router": "^2.7.0",
    "react-router-bootstrap": "^0.23.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1"
  },
  "devDependencies": {
    "standard": "^8.0.0"
  }
}
